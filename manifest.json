{
    "name": "Zeact",
    "description": "Base Level Extension",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["activeTab"],
    "action": {
      "default_popup": "hello.html",
      "default_icon": "zeact.png"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
        }
    ]
  }